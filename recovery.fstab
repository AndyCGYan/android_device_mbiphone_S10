# mount point       fstype    device                                                     flags
/system             ext4      /dev/block/by-name/system                                  flags=display="System";slotselect;backup=1;
/system_image       emmc      /dev/block/by-name/system                                  flags=display="System image";slotselect;backup=1;flashimg=1;
/vendor             ext4      /dev/block/by-name/vendor                                  flags=display="Vendor";slotselect;backup=1;
/vendor_image       emmc      /dev/block/by-name/vendor                                  flags=display="Vendor image";slotselect;backup=1;flashimg=1;
/data               ext4      /dev/block/by-name/userdata                                flags=display="Data";encryptable=footer;
/sdcard1            vfat      /dev/block/mmcblk1p1                                       flags=display="Sdcard1";
/boot               emmc      /dev/block/by-name/boot                                    flags=display="Boot";slotselect;backup=1;flashimg=1;
/misc               emmc      /dev/block/by-name/misc                                    flags=display="Misc";backup=1;flashimg=1;
